PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE record (
	description TEXT, 
	title TEXT, 
	price INTEGER, 
	author TEXT, 
	genre TEXT, 
	thumbnail_url TEXT, 
	year INTEGER, 
	id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, 
	quantity INTEGER
);
